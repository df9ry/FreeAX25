<Configuration name="Default"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://df9ry.ampr.org/FreeAX25/Config FreeAX25.xsd" 
	xmlns="http://df9ry.ampr.org/FreeAX25/Config">
	
	<Settings>
	</Settings>
	
	<Plugins>

		<!-- Builtin logger -->
		<Plugin name="_LOGGER">
			<Settings>
				<!-- NONE, ERROR, WARNING, INFO, DEBUG -->
				<Setting name="level">DEBUG</Setting>
			</Settings>
		</Plugin>
		
		<!-- Builtin timer -->
		<Plugin name="_TIMER">
			<Settings>
				<!-- Timer tick in ms -->
				<Setting name="tick">20</Setting>
			</Settings>
		</Plugin>
		
		<!-- Builtin null object -->
		<Plugin name="_NULL">
			<Instances>
				<Instance name="null">
					<ServerEndPoint name="downlink" url="channel:null"/>
				</Instance>
			</Instances>
		</Plugin>
		
	</Plugins>
	
</Configuration>
